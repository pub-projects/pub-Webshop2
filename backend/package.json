{
  "name": "shop-server",
  "version": "1.0.0",
  "description": "Backend for a webshop.",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon --exec ./node_modules/.bin/babel-node -r dotenv/config ./src/server.js",
    "test": "cross-env NODE_ENV=test npx mocha \"src/**/*.test.js\" --recursive --require @babel/register --file src/mocha-setup.js",
    "test:watch": "cross-env NODE_ENV=test npx mocha \"src/**/*.test.js\" --recursive --require @babel/register --file src/mocha-setup.js --watch",
    "coverage": "cross-env NODE_ENV=test nyc --reporter=lcov --reporter=text npm run test",
    "flow": "flow"
  },
  "author": "Chris Johannesson",
  "license": "ISC",
  "dependencies": {
    "@sendgrid/mail": "^7.6.0",
    "axios": "^0.24.0",
    "bcrypt": "^5.0.1",
    "dotenv": "^10.0.0",
    "express": "^4.17.2",
    "googleapis": "^92.0.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "mongodb": "^4.2.2",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.5",
    "@babel/node": "^7.16.5",
    "@babel/preset-env": "^7.16.5",
    "@babel/register": "^7.16.9",
    "chai": "^4.3.4",
    "cross-env": "^7.0.3",
    "flow-bin": "^0.169.0",
    "mocha": "^9.1.4",
    "nodemon": "^2.0.15",
    "nyc": "^15.1.0",
    "regenerator-runtime": "^0.13.9",
    "sinon": "^12.0.1",
    "supertest": "^6.2.1"
  }
